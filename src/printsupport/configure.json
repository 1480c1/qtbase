{
    "module": "printsupport",
    "depends": [
        "core",
        "gui",
        "widgets"
    ],
    "testDir": "../../config.tests",

    "commandline": {
        "options": {
            "cups": "boolean"
        }
    },

    "libraries": {
        "cups": {
            "description": "CUPS",
            "test": "unix/cups",
